<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.cordovaforspacebro" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cordova-for-spacebro</name>
    <description>
        A sample Apache Cordova application that displays the zeroconf/Bonjour services on iOS and Android.
    </description>
    <author email="alambrinidis@gmail.com" href="http://cordova.io">
      alambrinidis
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-zeroconf" spec="~1.2.1" />
    <plugin name="cordova-plugin-console" spec="~1.0.5" />
</widget>
