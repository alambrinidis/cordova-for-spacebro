<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.cordovaforspacebro" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="ZeroConf">
        <param name="android-package" value="net.becvert.cordova.ZeroConf" />
    </feature>
    <name>cordova-for-spacebro</name>
    <description>
        A sample Apache Cordova application that displays the zeroconf/Bonjour services on iOS and Android.
    </description>
    <author email="alambrinidis@gmail.com" href="http://cordova.io">
      alambrinidis
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
